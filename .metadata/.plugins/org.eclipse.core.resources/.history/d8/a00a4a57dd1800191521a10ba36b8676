package basics;



public class javapractice {
	
	int[] a;
	public static int methodB(int[] a) {
		int sum =5;
		for (int i=1; i<=3;i++) {
			sum=sum+a[i];
		}
		return sum;
		
		
		 
	 }
	public static void main(String[] args) {
		 
		int[] a= {5,2,3,4,1};
		System.out.print(methodB(a));
		
		   
	}


	

}
